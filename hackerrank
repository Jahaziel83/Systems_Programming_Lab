int main() 
{
char ch;            
char s[100];
char s1[122];
scanf("%c%*c",&ch);
scanf("%s%*c",&s);
scanf("%[^\n]",&s1);
printf("%c\n",ch);
printf("%s\n",s);
printf("%s",s1);
return 0;



int main()
{
	#include<stdio.h>


    int i, j;
    float f, g;
    scanf("%d %d %f %f", &i, &j, &f, &g);
    printf("%d %d\n%.1f %.1f", i+j, i-j, f+g, f-g);
    return 0;
}




#include <stdio.h>
 int max_of_four(int a, int b, int c, int d) {
    int max1 = a > b ? a : b;
    int max2 = c > d ? c : d;
    return max1 > max2 ? max1 : max2;
}







#include <stdio.h>

void update(int *a,int *b) {
    *a=*a+*b;
    *b=abs(*a-(2**b));    
}
